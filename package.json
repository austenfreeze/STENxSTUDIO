{
  "name": "stenxstudio",
  "description": "Next.js + Sanity: A Powerful Website Starter with Real-time Visual Editing",
  "private": true,
  "scripts": {
    "predev": "npm run typegen",
    "dev": "git add . && git commit -m \"Auto-commit: Work in progress before starting dev server.\" && npm-run-all --parallel dev:*",
    "dev:next": "npm run dev --workspace=nextjs-app",
    "prebuild": "npm run typegen && copy studio\\\\sanity.types.ts nextjs-app\\\\sanity.types.ts",
    "lint": "next lint",
    "dev:studio": "npm run dev --workspace=studio",
    "sync-env": "ts-node env.sync.ts",
    "postinstall": "npm prune --workspaces",
    "typegen": "npm run typegen --workspace=studio",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "workspaces": [
    "studio",
    "nextjs-app"
  ],
  "engines": {
    "node": "22.x"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.4.5",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@portabletext/react": "^3.2.1",
    "@sanity/client": "^6.29.1",
    "@sanity/icons": "^2.11.0",
    "@sanity/image-url": "^1.1.0",
    "@sanity/preview-kit": "^5.0.0",
    "@sanity/vision": "^4.3.0",
    "@sanity/visual-editing": "^2.0.22",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "autoprefixer": "^10.4.21",
    "esbuild": "^0.25.12",
    "next": "^15.4.2",
    "next-auth": "^5.0.0-beta.20",
    "next-auth-sanity": "^1.0.1",
    "next-sanity": "^10.0.6",
    "next-sanity-image": "^6.1.1",
    "postcss": "^8.5.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.2.1",
    "react-social-icons": "^6.16.0",
    "sanity": "^4.4.1",
    "server-only": "^0.0.1",
    "sonner": "^1.5.0",
    "styled-components": "^6.1.19",
    "tailwindcss": "^3.4.4"
  }
}
